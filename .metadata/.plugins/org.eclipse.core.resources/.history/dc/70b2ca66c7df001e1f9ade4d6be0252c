package com.krakedev;

public class TestCuadrado {

	public static void main(String[] args) {
		Cuadrado r1= new Cuadrado(6);
		Cuadrado p1= new Cuadrado(7);
		
		double area1;
		double perimetro1;
		
		r1.setLado(4);
		p1.setLado(3);
		
		area1=r1.calcularArea();
		perimetro1=p1.calcularPerimetro();
		
		System.out.println("Area de r1: "+area1);
		System.out.println("Perimetro de p1: "+perimetro1);

	}

}
